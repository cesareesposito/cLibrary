<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net6.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<PackageId>cLibrary</PackageId>
		<Version>1.1.1</Version>
		<Authors>Cesare Esposito</Authors>
		<Company></Company>		
		<Description>			 
			This library has been designed to improve development efficiency and simplify common tasks within your applications.
		</Description>
		<PackageReadmeFile>readme.md</PackageReadmeFile>
		<PackageLicenseFile>license.txt</PackageLicenseFile>
		<GeneratePackageOnBuild>True</GeneratePackageOnBuild>
		<PackageReleaseNotes>add Enumerable Extensions</PackageReleaseNotes>
	</PropertyGroup>

		<ItemGroup>

			<None Include="docs\readme.md" Pack="true" PackagePath="\" />
			<None Include="docs\license.txt" Pack="true" PackagePath="\" />
		
	</ItemGroup>

		<ItemGroup>
		  <PackageReference Include="EPPlus" Version="6.2.4" />
		  <PackageReference Include="Microsoft.EntityFrameworkCore" Version="7.0.5" />
		</ItemGroup>

</Project>
